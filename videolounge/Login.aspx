<%@ Page Language="C#" AutoEventWireup="true" CodeBehind="Login.aspx.cs" Inherits="videolounge.Login" %>

<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">

<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
<link href="css/stylesheet.css" rel="stylesheet" type="text/css" />
<link href="css/light/login-light.css" rel="stylesheet" type="text/css" />

    <title></title>
</head>
<body style="background-color:#D4BFBF">
    
    <form id="frmLogin" action="" class="login-form" runat="server">

    <asp:Image ID="imgLogo" ImageUrl="~/logo.PNG" runat="server"/><br /><br />

        <asp:Label ID="lblMessage" runat="server" Font-Bold="true" Font-Names="Calibri" ForeColor="Red"></asp:Label>

        <asp:TextBox ID="txtUsername" CssClass="login-input" runat="server" placeholder="Username"></asp:TextBox>
        <asp:TextBox ID="txtPassword" CssClass="login-input" runat="server" TextMode="Password" placeholder="Password"></asp:TextBox>

        <asp:Button ID="btnLogin" runat="server" Text="Login" CssClass="login-btn" onclick="btnLogin_Click"/>
    </form>

</body>
</html>
